{
  "name": "user-authentication-system",
  "version": "0.0.1",
  "description": "user-authentication-system initialised with create-express-typescript-application.",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "start": "node ./build/src/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "dev:nodemon": "nodemon",
    "test": "jest --collectCoverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src --quiet --fix",
    "lint:client": "eslint ./client/src --quiet --fix",
    "client": "npm start --prefix client",
    "run:fullstack": "concurrently \"npm run dev\" \"npm run client",
    "ts-node": "ts-node",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "generate:migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -n AddUsers",
    "run:migration": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "create:migration": "ts-node ./node_modules/typeorm/cli.js migration:create -n test",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config ./ormconfig.js",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "test:dev": "npm run lint && npm test && npm run schema:drop && npm run schema:sync && npm run seed:run && npm run dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:write && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/babel__core": "^7.1.14",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "joi-to-typescript": "^1.14.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.5",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/mysql": "^2.15.18",
    "@types/node": "^14.14.44",
    "@types/sequelize": "^4.28.9",
    "@types/shortid": "0.0.29",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "express-joi-validate": "^3.0.1",
    "express-joi-validation": "^5.0.0",
    "express-joi-validator": "^2.0.1",
    "faker": "^5.5.3",
    "fs": "0.0.1-security",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jest": "^26.6.3",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.2",
    "shortid": "^2.2.16",
    "supertest": "^6.1.3",
    "typeorm": "^0.2.32",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  }
}
